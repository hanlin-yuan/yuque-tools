{
  "extends": "@artus/tsconfig",
  "compilerOptions": {
    "module": "ES2022",
    "target": "ESNext",
    "outDir": "dist",
    "sourceMap": false,
    "inlineSourceMap": true,
    "noUnusedLocals": false,
    "moduleResolution": "node",
    "skipLibCheck": true,
    "declaration": false,
    "importHelpers": true,
    "esModuleInterop": true
    // "declarationDir": "./types"
  },

  "include": ["./src/**/*", "typings.d.ts", "./sdk.d.ts"]
}
